generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int       @id @default(autoincrement())
  email    String    @unique
  name     String?
  contacts Contact[]
}

model Contact {
  id     Int     @id @default(autoincrement())
  name   String
  phone  String
  image  String?
  User   User?   @relation(fields: [userId], references: [id])
  userId Int?
}
